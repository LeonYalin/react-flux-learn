{
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jquery": true
    },
    "globals": {
        "jQuery": true,
        "$": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "no-multi-assign": 0,
        "class-methods-use-this": 0,
        "no-alert": 0,
        "object-curly-newline": 0,
        "no-underscore-dangle": 0,
        "eslint linebreak-style": [0, "error", "windows"],
        "no-restricted-syntax": [2, "LabeledStatement", "WithStatement"],
        "new-cap": [2, { "newIsCapExceptions": ["autoComplete", "Router"] , "properties": false }],
        "max-len": [2, { "code": 130 }]
    },
    "plugins": ["json", "html", "react"]
}